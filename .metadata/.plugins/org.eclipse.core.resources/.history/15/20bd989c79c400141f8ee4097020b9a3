
public class p12 {
	String[] items = {"*-", "-***", "-*-*", "-**", "*", "**-*", "--*", "--*", "****", "**", "*---", "-*-", "*-**", "--", "-*", "---", "*--*", "--*-", "*-*", "***", "-", "**-", "***-", "*--", "-**-", "-*--", "--**"};
	public static void main(String[] args) {
		
	}
	public String codeVal(char c) {
		return items[c-'A'];
	}
}
