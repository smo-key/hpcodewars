import java.util.Scanner;


public class p12 {
	String[] items = {"*-", "-***", "-*-*", "-**", "*", "**-*", "--*", "--*", "****", "**", "*---", "-*-", "*-**", "--", "-*", "---", "*--*", "--*-", "*-*", "***", "-", "**-", "***-", "*--", "-**-", "-*--", "--**"};
	public static void main(String[] args) {
		Scanner a = r.rf("prob12.txt");
		while(r.hnl(a)) {
			String str = r.nl(a);
			StringBuilder sb = new StringBuilder(str);
		}
	}
	public String codeVal(char c) {
		return items[c-'A'];
	}
}
